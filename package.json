{
  "name": "greatshopper",
  "version": "0.0.1",
  "description": "A happy little Trump store.",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass  app/assets/main/ -o public/stylesheets",
    "test": "check-node-version --node '>= 6.7.0' && bin/setup && mocha app/**/*.test.js db/**/*.test.js server/**/*.test.js",
    "test-watch": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --watch app/**/*.test.js db/**/*.test.js server/**/*.test.js",
    "build": "check-node-version --node '>= 6.7.0' && bin/setup && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w",
    "start": "check-node-version --node '>= 6.7.0' && npm run seed && npm run build &&  npm run build-css && bin/setup && nodemon server/start.js",
    "start-dev": "check-node-version --node '>= 6.7.0' && bin/setup && nodemon server/start.js",
    "seed": "node db/seed.js"
  },
  "engines": {
    "node": "6.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queerviolet/bones.git"
  },
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "author": "Ashi Krishnan <me@ashi.works>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/queerviolet/bones/issues"
  },
  "homepage": "https://github.com/queerviolet/bones#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "cookie-session": "^2.0.0-alpha.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http": "0.0.0",
    "node-sass": "^3.10.1",
    "node-sass-middleware": "^0.9.8",
    "nodemailer": "^2.6.4",
    "nodemailer-smtp-transport": "^2.7.2",
    "nodemailer-wellknown": "^0.2.0",
    "nodemon": "^1.11.0",
